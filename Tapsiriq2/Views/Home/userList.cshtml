@model List<User> 
    <h1>User List</h1><hr />
    <a asp-action="creatUser" asp-controller="home" class="btn btn-primary">Create User</a>
    <hr />
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Email</th>
                <th>Password</th>
                <th>DateOfBirth</th>
                <th>Country</th>
                <th>City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.Email</td>
                    <td>@user.Password</td>
                    <td>@user.DateOfBirth.ToString("dd.MM.yyyy")</td>
                    <td>@user.Country</td>
                    <td>@user.City</td>
                </tr>
            }
        </tbody>
    </table>